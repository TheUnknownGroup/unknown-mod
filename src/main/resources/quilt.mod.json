{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${group}",
        "id": "${id}",
        "version": "${version}",
        "metadata": {
            "name": "${name}",
            "description": "${desc}",
            "contributors": {
                "TheGremlinX": "Owner"
            },
            "contact": {},
            "icon": "assets/unknown-mod/icon.png",
            "license": "MIT"
        },
        "intermediate_mappings": "net.fabricmc:intermediary",
        "entrypoints": {
            "init": "xyz.theunknowngroup.UnknownMod"
        },
        "depends": [
            {
                "id": "quilt_loader",
                "versions": ">=${loader}"
            },
            {
                "id": "quilted_fabric_api",
                "versions": ">=${api}"
            },
            {
                "id": "minecraft",
                "versions": ">=${mcver}"
            }
        ]
    },
    "mixin": "unknown-mod.mixins.json"
}
